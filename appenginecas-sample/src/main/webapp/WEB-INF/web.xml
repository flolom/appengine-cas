<?xml version="1.0" encoding="utf-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>CASLoginWS</servlet-name>
		<servlet-class>com.hotcocoacup.appenginecas.sample.CASLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CASLoginWS</servlet-name>
		<url-pattern>/casloginws</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MoodleCASLoginWS</servlet-name>
		<servlet-class>com.hotcocoacup.appenginecas.sample.MoodleCASLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MoodleCASLoginWS</servlet-name>
		<url-pattern>/moodlecasloginws</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
